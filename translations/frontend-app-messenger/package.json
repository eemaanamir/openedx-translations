{
  "name": "frontend-app-messenger",
  "version": "1.0.0",
  "description": "A messaging micro-frontend application for Wikilearn",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wikimedia/frontend-app-messenger.git"
  },
  "browserslist": [
    "extends @edx/browserslist-config"
  ],
  "scripts": {
    "build": "fedx-scripts webpack",
    "i18n_extract": "fedx-scripts formatjs extract",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress --host local.openedx.io",
    "start:with-theme": "paragon install-theme && npm start && npm install",
    "test": "fedx-scripts jest --coverage --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "homepage": "https://github.com/openedx/frontend-template-application#readme",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/openedx/frontend-template-application/issues"
  },
  "dependencies": {
    "@edx/brand": "npm:@openedx/brand-openedx@^1.2.2",
    "@edx/frontend-component-footer": "npm:@edly-io/wikimedia-frontend-component-footer@latest",
    "@edx/frontend-component-header": "^6.4.0",
    "@edx/frontend-platform": "^8.0.0",
    "@edx/openedx-atlas": "^0.6.0",
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "5.15.4",
    "@fortawesome/free-regular-svg-icons": "5.15.4",
    "@fortawesome/free-solid-svg-icons": "5.15.4",
    "@fortawesome/react-fontawesome": "0.2.2",
    "@openedx/paragon": "^22.0.0",
    "@reduxjs/toolkit": "^2.9.0",
    "bootstrap": "^4.6.2",
    "core-js": "3.41.0",
    "jquery": "^3.7.1",
    "lodash.camelcase": "^4.3.0",
    "lodash.snakecase": "^4.1.1",
    "multiselect-react-dropdown": "^2.0.25",
    "popper.js": "^1.16.1",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "^7.2.9",
    "react-router": "6.30.0",
    "react-router-dom": "6.30.0",
    "react-toastify": "^9.1.3",
    "redux": "4.2.1",
    "regenerator-runtime": "0.14.1"
  },
  "devDependencies": {
    "@edx/browserslist-config": "^1.1.1",
    "@edx/reactifex": "^2.1.1",
    "@openedx/frontend-build": "14.6.0",
    "glob": "7.2.3",
    "husky": "7.0.4",
    "jest": "29.7.0"
  }
}
